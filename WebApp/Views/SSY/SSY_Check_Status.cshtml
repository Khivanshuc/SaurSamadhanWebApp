                    <tbody>
                        <tr>
                            <td style="width:25%;"></td>

                            <td style="width:25%;font-weight: bolder; color:red;text-align: center" colspan="2">

                                STATUS - <span type="text" name="phase" id="phase"></span>
                            </td>
                            <td style="width:25%;"></td>

                        </tr>

                        <tr>
                            <td style="width:25%;font-weight: bold">Registration ID</td>
                            <td style="width:25%;font-weight: bold"><span id="appid" name="appid"></span></td>
                            <td style="width:25%;"> <span id="s_txtbfntype"></span> का नाम</td>
                            <td style="width:25%;"><span type="text" name="txtappname" id="txtappname">----</span></td>
                        </tr>
                        <tr>
                            <td id="divfathername">पिता/पति का नाम</td>
