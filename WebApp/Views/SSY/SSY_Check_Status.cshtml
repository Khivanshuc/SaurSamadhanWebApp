<head>
    <style type="text/css">
        body {
            background-image: url('../img/DB_Background.png');
        }
        .lblstatus {
            background: #e96060;
            cursor: default;
        }

        .lbloutput {
            text-transform: capitalize;
        }

        .row {
            padding: 5px 0;
        }

        .ui-datepicker .ui-datepicker-title select {
            font-size: 1em;
            margin: 1px 0;
            color: black;
        }

        #spinner {
            display: none;
            position: absolute;
            /* background:rgba(20,20,20,0.5); */
            width: 100%;
            height: 100%;
            padding-top: 20%;
            z-index: 1000;
        }

        #dttable th {
            background: #14b1bb;
            color: white;
        }

        td.details-control {
            background: url('images/btn_click_green2.png') no-repeat center center;
            cursor: pointer;
        }

        tr.shown td.details-control {
            background: url('images/btn_click_red2.png') no-repeat center center;
        }

        .link {
            min-width: 150px;
        }

        table td {
            text-transform: capitalize;
        }

        .modal {
            z-index: 10000
        }
    </style>

</head>

<body>

    <!-- ============ PAGE LOADER START ============ -->

    <div id="loader">
        <i class="fa fa-cog fa-4x fa-spin">
        </i>
    </div>
    <div id="spinner" class="spinner" align="center">
        <img id="img-spinner" src="images/loader1.gif" alt="Loading..."></img>
    </div>

    <section id="jobs" style="min-height: 525px;padding:0px">
        <div class="container">
