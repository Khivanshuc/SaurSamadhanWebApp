                        </div>
                        <div class="col-md-3 col-xs-6 col-lg-4">
                            <input id="mobileno" type="text" class="form-control" maxlength="10" minlength="6" placeholder="Mobile Nmber">
                            <label id="mobileError" style="color: red;"></label>
                        </div>
                        <div class="col-lg-2">
                            <button id="aadharSearch" class="btn btn-sm btn-info" style="height:37px;width:50%;" onclick="searchSSYDetail()"><i class="fa fa-search"></i> Search</button>
                        </div>
                        <div class="col-lg-2">
                            <a asp-controller="SSY" asp-action="SSY_Registration"><button id="btnBack" type="button" class="btn btn-sm btn-warning" style="height:37px;width:50%;margin-left:-90px;"><i class="fa fa-arrow-circle-o-left"></i> Back</button></a>
                        </div>
                    </div>
                </div>
            </div>


